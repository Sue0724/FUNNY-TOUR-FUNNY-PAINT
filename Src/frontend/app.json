    "pages/my_trips/my_trips",
    "pages/center/center",
    "pages/login/login",
    "pages/signup/signup",
    "pages/changeAvatar/changeAvatar",
    "pages/accountManage/accountManage",
    "pages/changePwd/changePwd",
    "pages/home/home",
