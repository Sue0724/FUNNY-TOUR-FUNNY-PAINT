    "pages/logs/logs",
    "pages/AI/AI",
    "pages/map/map_canvas/map_canvas",
    "pages/map/map_choose_places/map_choose_places",
    "pages/map/map_choose_places/place_detail/place_detail",
    "pages/map/trip/trip",
    "pages/myFriends/myFriends",
    "pages/addFriend/addFriend",
    "pages/messages/messages",
    "pages/giveComments",
    "pages/giveComments/giveComments"
